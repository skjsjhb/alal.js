{
  "private": true,
  "name": "alal.js",
  "author": "skjsjhb",
  "versionName": "SakuraKumo",
  "version": "2.0.1",
  "scripts": {
    "dev": "yarn bundle-debug --watch | yarn dev-server",
    "bundle": "yarn webpack --config webpack.config.ts",
    "bundle-debug": "cross-env NODE_ENV=development yarn bundle",
    "bundle-release": "cross-env NODE_ENV=production yarn bundle",
    "bundle-autotest": "cross-env NODE_ENV=autotest yarn bundle",
    "bundle-all": "yarn bundle-debug && yarn bundle-release && yarn bundle-autotest",
    "dev-server": "cross-env NODE_ENV=development yarn webpack serve -c webpack.config.ts",
    "make-autotest": "yarn electron-builder -c electron-builder.autotest.yml --publish=never",
    "make-release": "yarn electron-builder -c electron-builder.release.yml --publish=never",
    "make-debug": "yarn electron-builder -c electron-builder.debug.yml --publish=never",
    "make": "yarn make-debug && yarn make-release && yarn make-autotest",
    "launch-debug": "cd build/debug && npx electron .",
    "launch-release": "cd build/release && npx electron .",
    "launch-autotest": "cd build/autotest && npx electron .",
    "lint": "npx eslint",
    "test": "node tools/launchTest.js",
    "simple-test": "cross-env ALAL_TEST_SIMPLE=1 yarn test",
    "clean": "yarn rimraf build dist"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/eslint": "^8.56.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lzma-native": "^4.0.4",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/unzipper": "^0.10.9",
    "@types/uuid": "^9.0.7",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "copy-webpack-plugin": "^12.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.0",
    "electron": "^28.1.3",
    "electron-builder": "24.11.0",
    "eslint": "^8.56.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.1",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.9",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "fast-xml-parser": "^4.3.3",
    "fs-extra": "^11.2.0",
    "https-proxy-agent": "^7.0.2",
    "lzma-native": "^8.0.6",
    "nanoid": "^5.0.4",
    "node-fetch": "^3.3.2",
    "primeicons": "^6.0.1",
    "primereact": "^10.3.1",
    "proxy-agent": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "tiny-typed-emitter": "^2.1.0",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4"
  },
  "packageManager": "yarn@4.0.2"
}
