{
  "private": true,
  "appVersion": "SakuraKumo",
  "version": "1.0.0",
  "updatorVersion": 51,
  "scripts": {
    "bundle-debug": "yarn webpack --config webpack.config.debug.js",
    "bundle-release": "yarn webpack --config webpack.config.release.js",
    "bundle-autotest": "yarn webpack --config webpack.config.autotest.js",
    "bundle": "yarn bundle-debug && yarn bundle-release && yarn bundle-autotest",
    "make-autotest": "yarn electron-builder -c electron-builder.autotest.yml --publish=never",
    "make-release": "yarn electron-builder -c electron-builder.release.yml --publish=never",
    "make-debug": "yarn electron-builder -c electron-builder.debug.yml --publish=never",
    "make": "yarn make-debug && yarn make-release && yarn make-autotest",
    "launch-debug": "cd build/debug && npx electron .",
    "launch-release": "cd build/release && npx electron .",
    "launch-autotest": "cd build/autotest && npx electron .",
    "test": "node tools/launchTest.js",
    "clean": "rimraf build dist"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
